import java.util.Scanner;

public class Araaytext3 {
public static void main(String[] args) {
	Scanner scan = new Scanner(System.in);
	
	
	int[] arr=new int[10];
	System.out.println("10개의 정수를 입력하세요.");
	for(int i=0;i<10;i++) {
		arr[i]=scan.nextInt();
	}
	System.out.println();
	int[] num=new int[10];
	for(int i=0;i<10;i++) {
		if(arr[i]>0 && arr[i]<11) {
			num[0]++;
		}
		
		
		if(arr[i]>10 && arr[i]<21) {
			num[1]++;
		}
		
		if(arr[i]>20 && arr[i]<31) {
			num[2]++;
		}
		
		if(arr[i]>30 && arr[i]<41) {
			num[3]++;
		}
		
		if(arr[i]>40 && arr[i]<51) {
			num[4]++;
		}
		
		if(arr[i]>50 && arr[i]<61) {
			num[5]++;
		}
		
		if(arr[i]>60 && arr[i]<71) {
			num[6]++;
		}
		
		if(arr[i]>70 && arr[i]<81) {
			num[7]++;
		}
		
		if(arr[i]>80 && arr[i]<91) {
			num[8]++;
		}
		
		if(arr[i]>90 && arr[i]<101) {
			num[9]++;
		}
	}
	
	int a=1;
	int b=10;
	for(int i=0;i<10;i++) {
		System.out.println(a+"-"+b);
		for(int j=0;j<num[j];j++) {
			System.out.print("*");
		}
		a+=10;
		b+=10;
		System.out.println();
	}
	
	
}
}
