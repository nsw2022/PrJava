
public class Student extends Person{
	
	private String major="";
	
	@Override
	public void show() {
		// TODO Auto-generated method stub
		super.show();
		System.out.println("major: " + major);
	}
	public Student(){
		super();
		this.major="";
	}
	
	public Student(String name, int age, String major){
		super(name, age);
		this.major=major;
		
	}

}
