
public class Main {
	
	public static void main(String[] args) {
		
		//상속받은 기능이 마음에 들지 않을 때 사용하는 문법
		//Override
		
		Robot r = new Robot();
		r.move();
		r.attack();
		System.out.println();

		//이동,공격 + 나는 기능이 새로이 추가된 객체
		FlyRobot fr= new FlyRobot();
		fr.move();  // 상속받은기능.. 영.. 어올리지않음.. 오버라이드
		fr.attack();
		fr.fly();
		
	}

}
