import java.util.Scanner;

public class Araaytext {
public static void main(String[] args) {
	Scanner scan=new Scanner(System.in);
	
	
	int sum=0;
	
	
	int arr[]=new int[5];
	
	for(int i=0;i<5;i++) {
	System.out.println("정수를 입력해 주세요 : ");
	System.out.print("input : ");
	arr[i]=scan.nextInt();
	System.out.println(arr[i]);
	}
		
	
	
}
}
